    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-intl": "^6.1.1"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@types/react": "latest",
    "babel-plugin-formatjs": "^10.3.28",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "eslint-plugin-formatjs": "^4.3.1",
    "typescript": "latest"
  }
}
